{
  "name": "micro-frontend-app",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "eject": "react-scripts eject",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix",
    "start": "react-scripts start",
    "test": "react-scripts test --passWithNoTests --watchAll=false",
    "test:cov": "react-scripts test --coverage --passWithNoTests --watchAll=false",
    "test:watch": "react-scripts test",
    "test:e2e:push": "PORT=4100 start-server-and-test start 4100 cy:run",
    "test:e2e": "PORT=4100 start-server-and-test start 4100 cy:open",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@chakra-ui/react": "^1.7.4",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "apollo-boost": "^0.4.9",
    "framer-motion": "^5",
    "graphql": "^16.2.0",
    "lint-staged": ">=10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-spinners": "^0.11.0",
    "react-toastify": "^8.1.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "4.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "cypress": "^9.2.1",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "source-map-explorer": "^2.5.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-prettier": "^2.0.0",
    "tslint": "^6.1.2",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0"
  }
}
